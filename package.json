{
  "name": "callbreak",
  "version": "1.0.0",
  "description": "A fast-paced multiplayer Call Break card game built around local WiFi play",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "server": "ts-node server/index.ts",
    "server:build": "tsc -p tsconfig.server.json",
    "server:prod": "node build/index.js",
    "test": "jest",
    "lint": "eslint .",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "callbreak",
    "card-game",
    "multiplayer",
    "mobile"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "expo": "~50.0.0",
    "react": "18.2.0",
    "react-native": "0.73.0",
    "socket.io-client": "^4.7.2",
    "express": "^4.18.2",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "@types/react": "~18.2.45",
    "@types/react-native": "~0.73.0",
    "@types/express": "^4.17.21",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2"
  }
}
